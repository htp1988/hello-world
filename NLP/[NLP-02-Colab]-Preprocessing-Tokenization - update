   "source": [
    "PUNCT_TO_REMOVE = str(string.punctuation + string.digits)\n",
    "\n",
    "def remove_punc_and_num(text):\n",
    "    \"\"\"custom function to remove the punctuation\"\"\"\n",
    "    for token in PUNCT_TO_REMOVE:\n",
    "        text = text.replace(token, \"\")\n",
    "    return text"
   ]
  },
