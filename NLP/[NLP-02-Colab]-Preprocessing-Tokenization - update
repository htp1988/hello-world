    "#3.Standardizing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "LO44dly8ZQ8I"
   },
   "outputs": [],
   "source": [
    "dict_look = {'DM':'direct message', 'info':'information'}\n",
    "def stand_text(text):\n",
    "    for k, v in dict_look.items():\n",
    "      text = text.replace(k, v)\n",
    "    return text"
   ]
  },
